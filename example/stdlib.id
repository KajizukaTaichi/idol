func (range start end step) {
    let i = start;
    let list = [];
    while (start <= i < end) {
        let list = list + i;
        let i = i + step
    };
    (list)
};

func (len list) {
    let i = 0;
    for _ in list {
        let i = i + 1;
    };
    (i)
};

