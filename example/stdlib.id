func (range range.start range.end range.step) {
    let range.i = range.start;
    let range.result = [];
    while (range.start <= range.i < range.end) {
        let range.result = range.result + range.i;
        let range.i = range.i + range.step
    };
    (range.result)
};

func (slice slice.target slice.specified) {
    let slice.result = [];
    for slice.i in slice.specified {
        let slice.result = slice.result + (slice.target :: slice.i);
    };
    (slice.result)
};

func (len len.target) {
    let len.i = 0;
    for _ in len.target {
        let len.i = len.i + 1;
    };
    (len.i)
};

func ( puts puts.value ) {
    print { if ({ type of puts.value } == "text") puts.value else { cast puts.value to symbol } };
    print new-line
}
